public class PrimeFactor {

	public static void main(String[] args){
		long number=600851475143L;
		long i;

        for (i = 2; i <= number; i++) {
            if (number % i == 0) {
                number = number/ i;
                i--;
            }
        }
        
        System.out.println("largest: " + i);
	}
} 
